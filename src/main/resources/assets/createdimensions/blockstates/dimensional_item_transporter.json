{
  "multipart": [
    {
      "apply": {
        "model": "createdimensions:block/transporters/item/block"
      }
    },
    {
      "when": {
        "facing": "north"
      },
      "apply": {
        "model": "createdimensions:block/transporters/item/chest"
      }
    },
    {
      "when": {
        "facing": "east"
      },
      "apply": {
        "model": "createdimensions:block/transporters/item/chest",
        "y": 90
      }
    },
    {
      "when": {
        "facing": "south"
      },
      "apply": {
        "model": "createdimensions:block/transporters/item/chest",
        "y": 180
      }
    },
    {
      "when": {
        "facing": "west"
      },
      "apply": {
        "model": "createdimensions:block/transporters/item/chest",
        "y": 270
      }
    }
  ]
}